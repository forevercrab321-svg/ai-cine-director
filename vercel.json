{
    "framework": "vite",
    "buildCommand": "vite build",
    "outputDirectory": "dist",
    "functions": {
        "api/index.ts": {
            "maxDuration": 300
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        }
    ]
}